{
    "name": "xfxstudios/general",
    "type": "library",
    "description": "Libreria de Funciones Generales",
    "keywords": [
        "xfxstudios",
        "general"
    ],
    "homepage": "https://github.com/xfxstudios/general",
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Quintero",
            "email": "info.fxstudios@gmail.com",
            "homepage": "https://hitcel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4",
        "ramsey/uuid":"3.*",
        "geoip/geoip":"~1.16",
        "google/cloud-storage":"^1.5",
        "mailjet/mailjet-apiv3-php":"^1.0.9"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "xfxstudios\\general\\": "src",
            "xfxstudios\\Exception\\": "src/Exception"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "xfxstudios\\general\\": "tests",
            "xfxstudios\\Exception\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
